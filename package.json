    "url-loader": "~0.5.6",
    "webpack": "^2.2.0-rc.3",
    "webpack-dev-server": "^2.2.0-rc.0",
    "postcss-loader": "^1.3.3"
  },
  "repository": "LindaZhong/react-demo"

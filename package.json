{
  "name": "reactDemo",
  "version": "1.0.0",
  "description": "a collection of simple demos of react,Webpack,node",
  "main": "src/main.jsx",
  "keywords": [
    "react",
    "redux",
    "es6",
    "webpack",
    "node"
  ],
  "author": "Linda Zhong",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --devtool eval --progress --hot --content-base src",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "deploy-windows": "SET NODE_ENV=production & webpack -p --config webpack.production.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot",
    "pro": "cross-env NODE_ENV=production webpack --display-chunks --progress"
  },
  "dependencies": {
    "jquery": "3.x",
    "react": "15.x",
    "react-dom": "15.x",
    "react-redux": "^5.0.3",
    "react-router": "2.x",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "6.x",
    "babel-eslint": "6.x",
    "babel-loader": "6.x",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "6.x",
    "babel-preset-stage-2": "^6.22.0",
    "bundle-loader": "^0.5.4",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "css-loader": "0.x",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "0.x",
    "node-sass": "^4.5.0",
